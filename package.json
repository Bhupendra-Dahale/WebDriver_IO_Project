{
  "name": "webdriver_io_project",
  "version": "1.0.0",
  "description": "webdriver_IO tests",
  "main": "index.js",
  "scripts": {
    "test": "Hello WDIO..!!",
    "wdio": "wdio run ./wdio.conf.ts",
    "debug": "set DEBUG=Y&& set HEADLESS=Y&& wdio run wdio.conf.ts --cucumberOpts.tagExpression='@demo'",
    "demo": "set HEADLESS=Y&& wdio run wdio.conf.ts --cucumberOpts.tagExpression='@demo'",
    "web": "set HEADLESS=Y&& wdio run wdio.conf.ts --cucumberOpts.tagExpression='@web'",
    "invent": "set HEADLESS=Y&& wdio run wdio.conf.ts --cucumberOpts.tagExpression='@invent'",
    
    "Test": "wdio run ./wdio.conf.ts --suite Test",

    "test:invent": "set HEADLESS=N&& wdio run config/wdio.test.conf.ts --cucumberOpts.tagExpression='@invent'",

    "hrm": "set HEADLESS=N&& wdio run ./wdio.conf.ts --suite OrangeHRM"
  },
  "keywords": [
    "webdriverIO",
    "cucumber",
    "e2e",
    "tests"
  ],
  "author": "Bhupendra Dahale",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@wdio/allure-reporter": "^8.14.0",
    "@wdio/cli": "^8.14.1",
    "@wdio/cucumber-framework": "^8.14.0",
    "@wdio/local-runner": "^8.14.1",
    "@wdio/spec-reporter": "^8.14.0",
    "chai": "^4.3.7",
    "chromedriver": "^115.0.0",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-vscode-service": "^5.2.1",
    "winston": "^3.10.0"
  },
  "dependencies": {
    "@cucumber/cucumber": "^9.3.0"
  }
}
